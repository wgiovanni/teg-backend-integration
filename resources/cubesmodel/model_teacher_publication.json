{
    "dimensions": [
        {
         "name":"dim_docente",
         "levels": [
                {
                    "name":"docente",
                    "label":"Docente",
                    "attributes": ["id", "cedula", "nombre", "apellido", "correo", "area_trabajo"],
                    "label_attribute": "nombre"
                }
          ]
        },
        {
          "name":"dim_facultad",
          "levels": [
            {
              "name": "facultad",
              "label":"Facultad",
              "attributes": ["id", "nombre"],
              "label_attribute": "nombre"
            }
          ]
        },
        {
            "name": "dim_publicacion",
            "levels": [
              {
                "name": "publicacion",
                "label": "Publicacion",
                "attributes": ["id", "autor", "titulo", "codigo", "fecha", "revista"],
                "label_attribute": "titulo"
              }
            ]
          }
    ],
    "cubes": [
        {
            "name": "fact_docente_publicacion",
            "dimensions": ["dim_docente", "dim_facultad", "dim_publicacion"],
            "measures": [
              {"name": "cantidad", "label": "Cantidad"},
              {"name": "cantidad_citas","label": "Citas"}
            ],
            "aggregates": [
              {
                "name": "sumatoria",
                "function": "count",
                "measure": "cantidad"
              },
              {
                "name": "sumatoria_citacion",
                "function": "sum",
                "measure": "cantidad_citas"
              }
            ],
            "joins": [
              {
                "master": "fact_docente_publicacion.id_docente",
                "detail": "dim_docente.id"
              },
              {
                "master": "fact_docente_publicacion.id_facultad",
                "detail": "dim_facultad.id"
              },
              {
                "master": "fact_docente_publicacion.id_publicacion",
                "detail": "dim_publicacion.id"
              }
            ],
            "mappings": {
              "dim_docente.id": "dim_docente.id",
              "dim_docente.cedula": "dim_docente.cedula",
              "dim_docente.nombre": "dim_docente.nombre",
              "dim_docente.apellido": "dim_docente.apellido",
              "dim_docente.area_trabajo": "dim_docente.area_trabajo",
              "dim_docente.correo": "dim_docente.correo",
              "dim_facultad.id": "dim_facultad.id",
              "dim_facultad.nombre": "dim_facultad.nombre",
              "dim_publicacion.id": "dim_publicacion.id",
              "dim_publicacion.codigo": "dim_publicacion.codigo",
              "dim_publicacion.autor": "dim_publicacion.autor",
              "dim_publicacion.titulo": "dim_publicacion.titulo",
              "dim_publicacion.fecha": "dim_publicacion.fecha",
              "dim_publicacion.revista": "dim_publicacion.revista"
            }
        }
    ]
}